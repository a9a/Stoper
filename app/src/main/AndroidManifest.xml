        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuStart"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_menu_start" >
        </activity>
        <activity
            android:name=".Wyniki"
            android:label="@string/title_activity_wyniki" >
        </activity>
        <activity
            android:name=".CzasItem"
            android:label="@string/title_activity_czas_item" >
        </activity>
        <activity
            android:name=".WyswietlanieWyniku"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_wyswietlanie_wyniku" >
        </activity>
    </application>

</manifest>
